<div class="page-header">
  <h1>My posts</h1>
</div>

<%= will_paginate %>

<div id="my-posts">
  <%= render @posts %>
</div>

<% if @posts.next_page %>
    <div id="with-button">
      <%= will_paginate %>
    </div>

    <div id="load_more_posts" class="btn btn-primary btn-lg">More posts</div>
<% end %>